{
  "name": "connection-utility",
  "version": "1.0.0",
  "main": "src/main.js",
  "scripts": {
    "app": "webpack && electron .",
    "dev": "webpack",
    "start": "webpack-dev-server",
    "electron": "electron .",
    "build:linux": "build --platform linux",
    "build:win": "npm run build:win64 && npm run build:win32",
    "build:win32": "build --platform win32 --arch ia32",
    "build:win64": "build --platform win32 --arch x64"
  },
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "bonjour": "3.5.0",
    "multicast-dns": "github:alexey-martynov/multicast-dns#multihomed-multicast",
    "bootstrap": "^4.0.0-beta",
    "cross-env": "^1.0.8",
    "expect": "^1.20.1",
    "jquery": "^3.2.1",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.7.2",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.6.2",
    "react-localization": "^1.0.10",
    "react-popper": "^0.7.4",
    "react-redux": "^4.4.5",
    "react-router-dom": "latest",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "semantic-ui-react": "^0.83.0",
    "style-loader": "^0.13.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.11.0"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "electron": "^2.0.5",
    "electron-builder": "^20.26.0",
    "electron-builder-squirrel-windows": "^19.18.0",
    "electron-squirrel-startup": "^1.0.0",
    "json-loader": "^0.5.7",
    "postcss-loader": "^2.0.8",
    "react-transform-hmr": "^1.0.4",
    "webpack": "^1.15.0"
  },
  "build": {
    "productName": "ConnectionUtility",
    "asar": "true",
    "extends": null,
    "squirrelWindows": {
      "msi": false
    },
    "nsis": {
      "oneClick": "true",
      "perMachine": "true",
      "allowElevation": "true"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
