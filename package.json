{
  "name": "connection-utility",
  "version": "1.0.0",
  "main": "src/main.js",
  "scripts": {
    "app": "webpack && electron .",
    "dev": "webpack",
    "start": "webpack-dev-server",
    "electron": "electron .",
    "build:linux": "build --platform linux",
    "build:win": "npm run build:win64 && npm run build:win32",
    "build:win32": "build --platform win32 --arch ia32",
    "build:win64": "build --platform win32 --arch x64",
    "build:mac": "build --platform mac",
    "prettiercheck": "prettier --check \"src/app/**/*.js\"  \"src/app/**/**/*.js\" \"src/main.js\" \"src/test/*.js\" \"src/test/**/*.js\" \"testcafe/*.js\" \"testcafe/helpers/*.js\"",
    "prettierwrite": "prettier --write \"src/app/**/*.js\"  \"src/app/**/**/*.js\" \"src/main.js\" \"src/test/*.js\" \"src/test/**/*.js\" \"testcafe/*.js\" \"testcafe/helpers/*.js\"",
    "prettier": "prettier",
    "test": "mocha --opts ./src/test/mocha.opts 'src/test/**/*.js' 'src/test/**/**/*.js'",
    "testcafe": "testcafe electron \"testcafe/*.js\" "
  },
  "license": "ISC",
  "dependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "6.4.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "bonjour": "3.5.0",
    "bootstrap": "4.0.0",
    "chai": "4.2.0",
    "cross-env": "1.0.8",
    "expect": "1.20.2",
    "jquery": "3.4.1",
    "multicast-dns": "github:alexey-martynov/multicast-dns#multihomed-multicast",
    "node-libs-browser": "2.1.0",
    "node-sass": "4.9.4",
    "react": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-bootstrap": "0.32.4",
    "react-dom": "15.6.2",
    "react-localization": "1.0.10",
    "react-popper": "0.7.4",
    "react-redux": "4.4.9",
    "react-router-dom": "5.1.2",
    "react-transition-group": "2.5.0",
    "reactjs-mappletooltip": "1.4.63",
    "redux": "3.7.2",
    "redux-logger": "2.10.2",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.3.0",
    "sass-loader": "4.1.1",
    "sinon": "8.0.1",
    "style-loader": "0.13.2",
    "testcafe": "1.8.1",
    "testcafe-browser-provider-electron": "0.0.14",
    "testcafe-react-selectors": "4.0.0",
    "webpack-dev-middleware": "1.12.2",
    "webpack-hot-middleware": "2.24.3"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "0.23.1",
    "electron": "2.0.18",
    "electron-builder": "20.34.0",
    "electron-builder-squirrel-windows": "19.56.0",
    "electron-squirrel-startup": "1.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-15": "1.4.1",
    "jsdom": "15.2.1",
    "jsdom-global": "3.0.2",
    "json-loader": "0.5.7",
    "mocha": "6.2.2",
    "postcss-loader": "2.1.6",
    "prettier": "1.19.1",
    "react-test-renderer": "15.6.2",
    "react-transform-hmr": "1.0.4",
    "webpack": "1.15.0"
  },
  "prettier": {
    "bracketSpacing": false,
    "endOfLine": "auto",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "parser": "typescript",
    "printWidth": 140,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
  },
  "build": {
    "productName": "DiscoveryUtility",
    "asar": "true",
    "extends": null,
    "squirrelWindows": {
      "msi": false
    },
    "files": [
      "src"
    ],
    "asarUnpack": [
      "src/cert.exe"
    ],
    "nsis": {
      "oneClick": "true",
      "perMachine": "true",
      "allowElevation": "true"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    }
  }
}
