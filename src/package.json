{
  "name": "react_electron_app",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "author": {
    "name": "ConnectionUtility",
    "email": "support@TBD.com"
  },
  "dependencies": {
    "bonjour": "3.5.0",
    "multicast-dns": "github:alexey-martynov/multicast-dns#multihomed-multicast",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "build:linux": "build --platform linux",
    "build:win": "npm run build:win64 && npm run build:win32",
    "build:win32": "build --platform win32 --arch ia32",
    "build:win64": "build --platform win32 --arch x64",
    "pack": "build --dir"
  },
  "devDependencies": {
    "electron": "^2.0.5",
    "electron-builder": "^20.26.0",
    "electron-builder-squirrel-windows": "^19.18.0",
    "electron-squirrel-startup": "^1.0.0"
  },
  "build": {
    "productName": "ConnectionUtility",
    "asar": "true",
    "extends": null,
    "squirrelWindows": {
      "msi": false
    },
    "nsis": {
      "oneClick": "true",
      "perMachine": "true",
      "allowElevation": "true"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
